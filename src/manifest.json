{
  "manifest_version": 2,
  "name": "Fallen London Sidebar Outfit",
  "description": "This extension does something.",
  "permissions": [
    "webRequest",
    "storage",
    "*://*.fallenlondon.com/"
  ],
  "version": VERSION,
  "content_scripts": [
    {
      "matches": [
        "http://fallenlondon.storynexus.com/Gap/Load*",
        "https://*.fallenlondon.com/*",
        "http://localhost:3000/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "content-script.js"
      ],
      "run_at": "document_end"
    }
  ]
}
